#include <stdlib.h>

int     ft_ultimate_range(int **range, int min, int max)
{
        int     i;

        i = 0;
        if (min >= max)
        {
                *range = NULL;
                return (0);
        }
        *range = malloc(sizeof(int) * (max - min));
        if (*range == NULL)
                return (-1);
        while (min < max)
        {
                (*range)[i] = min;
                min++;
                i++;
        }
        return (i);
}


#include <stdio.h>

int     main(void)
{
        int     i;

        i = ft_ultimate_range (0,1,9);
        printf("size: %d\n", i);
        return(0);
}
